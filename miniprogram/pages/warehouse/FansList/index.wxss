/* pages/warehouse/FansList.wxss */
page {
  background-color: #f6f6f6;
}

.fans_list_item {
  display: flex;
  align-items: center;
  padding: 20rpx;
  background-color: #fff;
  color: #333;
  border-bottom: 1px solid #f6f6f6;
}

.fans_list_item:last-child {
  border-bottom: none;
}

.fans_list_item_avatar image {
  width: 70rpx;
  height: auto;
  display: block;
  border-radius: 50%;
  margin-right: 10rpx;
}

.fans_lsit_item_nick_name {
  flex: 1;
}

.default_page {
  height: 100vh;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #666;
  font-size: 28rpx;
}

.fans_lsit_item_time {
  font-size: 24rpx;
  color: #666;
}